  </data>
  <data name="ROU_DebugCommandsDisabled" xml:space="preserve">
    <value>Disabled</value>
  </data>
  <data name="ROU_DebugCommandsInvincibility" xml:space="preserve">
    <value>Invincibility</value>
  </data>
  <data name="ROU_DebugCommandsInvincibilityInfo" xml:space="preserve">
    <value>This allows the player to not die when taking damage</value>
  </data>
  <data name="ROU_DebugCommandsHideMouse" xml:space="preserve">
    <value>Hide mouse pointer</value>
  </data>
  <data name="ROU_DebugCommandsHideMouseInfo" xml:space="preserve">
    <value>This will hide the mouse pointer in the game menus</value>
  </data>
  <data name="ROU_DebugCommandsMaxZoom" xml:space="preserve">
